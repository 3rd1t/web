{
  "version": 1,
  "projects": {
    "research": {
      "root": "packages/research",
      "sourceRoot": "packages/research",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "packages/research",
            "outputPath": "dist/packages/research"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "research:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "research:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "research:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["packages/research/tsconfig.json", "packages/research/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!packages/research/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/research/jest.config.js",
            "tsConfig": "packages/research/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@netlify-builder/deploy:deploy",
          "options": {
            "outputPath": "dist/packages/research/exported",
            "siteId": "abfda65d-dcb7-4d66-90d2-352b9ea50dc1"
          }
        }
      }
    },
    "research-e2e": {
      "root": "packages/research-e2e",
      "sourceRoot": "packages/research-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/research-e2e/cypress.json",
            "tsConfig": "packages/research-e2e/tsconfig.e2e.json",
            "devServerTarget": "research:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "research:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["packages/research-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!packages/research-e2e/**/*"]
          }
        }
      }
    },
