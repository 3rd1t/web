{
  "version": 1,
  "projects": {
    "research": {
      "root": "apps/research",
      "sourceRoot": "apps/research",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/research",
            "outputPath": "dist/apps/research"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "research:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "research:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "research:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/research/tsconfig.json", "apps/research/tsconfig.test.json"],
            "exclude": ["**/node_modules/**", "!apps/research/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/research/jest.config.js",
            "tsConfig": "apps/research/tsconfig.test.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "research-e2e": {
      "root": "apps/research-e2e",
      "sourceRoot": "apps/research-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/research-e2e/cypress.json",
            "tsConfig": "apps/research-e2e/tsconfig.e2e.json",
            "devServerTarget": "research:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "research:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/research-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/research-e2e/**/*"]
          }
        }
      }
    },
    "components": {
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["libs/components/tsconfig.lib.json", "libs/components/tsconfig.test.json"],
            "exclude": ["**/node_modules/**", "!libs/components/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "tsConfig": "libs/components/tsconfig.test.json",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/components",
            "config": {
              "configFolder": "libs/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "core": {
      "root": "apps/core",
      "sourceRoot": "apps/core",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/core",
            "outputPath": "dist/apps/core"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "core:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "core:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "core:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/core/tsconfig.json", "apps/core/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/core/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/core/jest.config.js",
            "tsConfig": "apps/core/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-e2e": {
      "root": "apps/core-e2e",
      "sourceRoot": "apps/core-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/core-e2e/cypress.json",
            "tsConfig": "apps/core-e2e/tsconfig.e2e.json",
            "devServerTarget": "core:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "core:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/core-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/core-e2e/**/*"]
          }
        }
      }
    },
    "project": {
      "root": "apps/project",
      "sourceRoot": "apps/project",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/project",
            "outputPath": "dist/apps/project"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "project:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "project:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "project:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/project/tsconfig.json", "apps/project/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/project/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/project/jest.config.js",
            "tsConfig": "apps/project/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "project-e2e": {
      "root": "apps/project-e2e",
      "sourceRoot": "apps/project-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/project-e2e/cypress.json",
            "tsConfig": "apps/project-e2e/tsconfig.e2e.json",
            "devServerTarget": "project:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "project:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/project-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/project-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "research"
}
